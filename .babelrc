
{
    "presets": [
      ["env", {
        "modules": false,
        "targets": {
          "browsers": ["> 2%", "last 2 versions", "not ie <= 8"]
        }
      }],
      "stage-2",
      "vue"
    ],
    "plugins": [
        "transform-vue-jsx", "transform-runtime", "transform-regenerator",
        ["transform-imports", {
            "vuetify": {
              "transform": "vuetify/es5/components/${member}",
              "preventFullImport": true
            }
        }]
    ],
    "env": {
      "test": {
        "presets": ["env", "stage-2"],
        "plugins": ["transform-vue-jsx", "transform-es2015-modules-commonjs", "dynamic-import-node", "syntax-dynamic-import", "babel-plugin-dynamic-import-webpack"]
      }
    }
  }